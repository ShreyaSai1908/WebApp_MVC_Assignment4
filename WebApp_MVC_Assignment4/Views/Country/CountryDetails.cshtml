@model WebApp_MVC_Assignment4.Models.ViewModels.CreateCountryViewModel

<div>
    <h4>Country Details</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CountryId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CountryId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CountryName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CountryName)
        </dd>
    </dl>
    <div>
       <h4>Details</h4>
       <ul>
           @foreach (var item in Model.CityList)
            {
               <li>@item.CityName</li>
            }
       </ul>
    </div>
</div>
<div>   
    <a asp-action="ShowCountry">Back to List</a>
</div>
